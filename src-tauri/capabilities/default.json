{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "stronghold:default",
    "store:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/sldl",
          "sidecar": true
        }
      ]
    },
    "shell:allow-open",
    "shell:default"
  ]
}